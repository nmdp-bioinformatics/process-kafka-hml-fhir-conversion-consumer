    <groupId>org.nmdp</groupId>
    <artifactId>process-kafka-hml-fhir-conversion-consumer</artifactId>
    <packaging>jar</packaging>
    <version>1.0.0-SNAPSHOT</version>

    <name>process-kafka-hml-fhir-conversion-consumer</name>
    <description>Kafka Message Consumer for Hml to Fhir Conversion</description>

    <properties>
        <apache-kafka-version>0.10.2.1</apache-kafka-version>
        <slf4j-version>1.7.6</slf4j-version>
        <log4j-version>2.8.2</log4j-version>
        <metrics-version>3.1.2</metrics-version>
        <java.version>1.8</java.version>
        <snake-yaml-version>1.16</snake-yaml-version>
        <jackson-xml-version>2.2.3</jackson-xml-version>
        <nmdp-consumer-version>1.2.0</nmdp-consumer-version>
        <nmdp-models-version>1.8.2</nmdp-models-version>
        <nmdp-models-api-version>1.18.0</nmdp-models-api-version>
        <google-guice-version>4.0</google-guice-version>
        <gson-version>2.8.0</gson-version>
        <maven-compiler-plugin-version>3.5.1</maven-compiler-plugin-version>
        <commons-logging-api-version>1.1</commons-logging-api-version>
        <guava-version>12.0.1</guava-version>
        <nmdp-conversion-version>1.3.30</nmdp-conversion-version>
        <model-mapper-version>0.7.5</model-mapper-version>
        <joda-time-version>2.9.9</joda-time-version>
        <mongodb-driver-version>3.4.0-beta1</mongodb-driver-version>
        <json-version>20160212</json-version>
        <nmdp-mongo-version>1.1.0</nmdp-mongo-version>
        <commons-lang-version>3.1</commons-lang-version>
    </properties>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>${maven-compiler-plugin-version}</version>
                <configuration>
                    <source>${java.version}</source>
                    <target>${java.version}</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
    
